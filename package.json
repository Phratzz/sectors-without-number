{
  "name": "sectors-without-number",
  "version": "7.7.2",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpigsley/sectors-without-number"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "start:prod": "sh -ac '. .env.prod; npm start'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy --except functions",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:prod": "firebase use prod && sh -ac '. .env.prod; npm run deploy'",
    "deploy:prod:functions": "firebase use prod && sh -ac '. .env.prod; npm run deploy:functions'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "translate": "node ./src/utils/convert-translations.js"
  },
  "dependencies": {
    "chance": "^1.0.16",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.4.0",
    "copy-to-clipboard": "^3.0.8",
    "dayjs": "^1.6.10",
    "firebase": "^5.7.0",
    "history": "^4.7.2",
    "is-url-external": "^1.0.3",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-fastclick": "^3.0.2",
    "react-feather": "^1.1.0",
    "react-hint": "^3.1.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-markdown": "^3.3.4",
    "react-measure": "^2.1.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.3.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "firebase-tools": "^6.1.2",
    "prettier": "^1.14.3",
    "react-scripts": "2.0.4",
    "react-test-renderer": "^16.5.2",
    "source-map-explorer": "^1.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules",
      "!src/components/**/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
