{
  "name": "sectors-without-number",
  "version": "7.15.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpigsley/sectors-without-number"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "start:prod": "sh -ac '. .env.prod; npm start'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy --except functions",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:prod": "firebase use prod && sh -ac '. .env.prod; npm run deploy'",
    "deploy:prod:functions": "firebase use prod && sh -ac '. .env.prod; npm run deploy:functions'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "translate": "node ./src/utils/convert-translations.js"
  },
  "dependencies": {
    "chance": "^1.0.18",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4.0",
    "copy-to-clipboard": "^3.2.0",
    "dayjs": "^1.8.14",
    "firebase": "^6.2.3",
    "history": "^4.9.0",
    "is-url-external": "^1.0.3",
    "lodash": "^4.17.14",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-color": "^2.17.3",
    "react-dom": "^16.10.1",
    "react-fastclick": "^3.0.2",
    "react-feather": "^1.1.6",
    "react-hint": "^3.2.0",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.1.0",
    "react-measure": "^2.3.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.0",
    "react-redux-toastr": "^7.5.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "firebase-tools": "^6.10.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.17.1",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.5.2",
    "source-map-explorer": "^1.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules",
      "!src/components/**/index.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
